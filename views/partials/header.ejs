<meta charset="utf-8">
<meta name="viewport" content="width=device-width">

<title>Blairpath.org |</title>
<link rel="shortcut icon" href="/favicon.ico">
<script>
	const PAGE_NAMES = ['Map', 'Tutorial', 'About'];
	const getPath = name => '/' + name.toLowerCase();
	var page_name = '';
	PAGE_NAMES.forEach(name => {
		if (getPath(name) == window.location.pathname) page_name = name;
	});
	document.title = page_name || '404';
</script>